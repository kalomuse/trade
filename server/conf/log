create table account(
	id int(11) unsigned not null auto_increment, 
	account varchar(64),
	password varchar(64),
	site_name varchar(64),
	site_url varchar(128),
	created_time datetime default CURRENT_TIMESTAMP,
	modified_time datetime default CURRENT_TIMESTAMP,
	deleted int(4) unsigned default 0,
	mark varchar(128),
	primary key(id));
create table product(
    id int(11) unsigned not null auto_increment,
    name varchar(64),
    category text,
    price int(10),
    brief varchar(128),
    description text,
    img varchar(128),
    count int(8) defalut 0,
    created_time datetime default CURRENT_TIMESTAMP,
    modified_time datetime default CURRENT_TIMESTAMP,
    deleted int(4) unsigned default 0,
    primary key(id));

    create table prize(
            id int(11) unsigned not null auto_increment,
            open_id varchar(64),
            prize_id int(6),
            prize_name varchar(128),
            mobile varchar(32),
            name varchar(32),
            code varchar(32),
            created_time TIMESTAMP  default CURRENT_TIMESTAMP,
            deleted int(4) unsigned default 0,
            primary key(id));
    create table user_info(
        id int(11) unsigned not null auto_increment,
        unionid varchar(64),
        openid varchar(64),
        nickname varchar(32),
        sex int(4),
        city varchar(32),
        province varchar(32),
        country varchar(32),
        headimgurl text,
        subscribe_time varchar(11),
        created_time TIMESTAMP  default CURRENT_TIMESTAMP,
        deleted int(4) unsigned default 0,
        primary key(id));
     );
        create table user_point(
            id int(11) unsigned not null auto_increment,
            open_id varchar(64),
            scene_id varchar(64),
            ticket varchar(256),
            point int(11),
            mobile int(12),
            name varchar(32),
            code varchar(32),
            expire_time int(10),
            created_time TIMESTAMP  default CURRENT_TIMESTAMP,
            deleted int(4) unsigned default 0,
            primary key(id));

    id open_id scene_id point expire_time deleted create_time modify_time

   create table log(
       id int(12) unsigned not null auto_increment,
       site_name varchar(64),
       site_url varchar(128),
       product_name varchar(64),
       product_id int(8),
       success int(2),
       msg text,
       created_time datetime default CURRENT_TIMESTAMP,
       modified_time datetime default CURRENT_TIMESTAMP,
       deleted int(4) unsigned default 0,
       primary key(id));


        insert into user set(account, password, email) value('111111', '111111', '111111@qq.com');
       create table user(
       	id int(11) unsigned not null auto_increment,
       	account varchar(128),
       	password varchar(128),
       	email varchar(128),
       	company_name varchar(128),
       	company_en_name varchar(128),
       	province varchar(16),
       	city varchar(16),
       	district varchar(16),
        contact_name varchar(32),
        contact_phone varchar(32),
        boss_name varchar(32),
       	company_number varchar(64),
       	company_number_img varchar(128),
       	reg_email varchar(128),
       	reg_account varchar(128),
       	reg_password varchar(128),
       	created_time datetime default CURRENT_TIMESTAMP,
       	modified_time datetime default CURRENT_TIMESTAMP,
       	deleted int(4) unsigned default 0,
       	primary key(id));
